<!DOCTYPE html>
<html lang="en">

<!-- @import "components/head.html" -->

<body tabIndex="0">
  <!-- @import "components/header.html" -->
  <main>
    <section class="orders profile page-padding">
      <div class="container">
        <h1 class="page-title orders__title">Orders</h1>
        <div class="profile__body">
          <nav class="profile__nav" aria-label="breadcrumb">
            <ul class="profile__links">
              <li class="profile__link">
                <a href="#profile">Profile</a>
              </li>
              <li class="profile__link">
                <a href="#orders" aria-current="page">Orders</a>
              </li>
            </ul>
          </nav>
          <div class="orders__table table">
            <ul class="table__headers">
              <li class="table__header">Goods</li>
              <li class="table__header">Total</li>
              <li class="table__header">Status</li>
              <li class="table__header">Order date</li>
              <li class="table__header">Delivery date</li>
              <li class="table__header">Order number</li>
              <li class="table__header">Tracking number</li>
            </ul>
            <p class="notification hidden">No orders</p>
            <ul class="table__items">
              <li class="table__item">
                <div class="cell image">
                  <img src="img/product-face-cream-1.jpg" alt="">
                </div>
                <span class="cell price">$ 160.00</span>
                <span class="cell status">Submitted</span>
                <span class="cell order-date">05.02.2021</span>
                <span class="cell delivery-date">10.02.2021</span>
                <span class="cell order-number">476502291</span>
                <span class="cell tracking-number">476502291</span>
              </li>
              <li class="table__item">
                <div class="cell image">
                  <img src="img/product-face-cream-1.jpg" alt="">
                </div>
                <span class="cell price">$ 160.00</span>
                <span class="cell status">Submitted</span>
                <span class="cell order-date">05.02.2021</span>
                <span class="cell delivery-date">10.02.2021</span>
                <span class="cell order-number">476502291</span>
                <span class="cell tracking-number">476502291</span>
              </li>
            </ul>
          </div>
        </div>
      </div>
    </section>
  </main>
