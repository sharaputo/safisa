                  </li>
                  <li class="contact__input">
                    <label for="phone" class="screen-reader">Telephone</label>
                    <input type="tel" class="error" id="phone" name="tel" placeholder="Telephone *" required>
                  </li>
                  <li class="contact__input">
                    <label for="email" class="screen-reader">Email</label>
                    <input type="email" id="email" name="email" placeholder="email *" required>
                  </li>
                </ul>
